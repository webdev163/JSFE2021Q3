  "scripts": {
    "start": "gulp",
    "gulp": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production webpack --mode production --json > utils/stats.json && webpack-bundle-analyzer utils/stats.json build --report utils/report.html --mode static -O && cross-env NODE_ENV=production gulp build",
    "avif": "avif --input='src/assets/img/*.{jpg,jpeg,png}' --output='build/img' --quality=90 --overwrite --verbose && npx avif --input='src/assets/img/_copy/*.{jpg,jpeg,png}' --output='build/img' --quality=90 --overwrite --verbose",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "lint": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "stylelint": "stylelint --aei --fix \"src/**/*.scss\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage"
  },
