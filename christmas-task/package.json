{
  "name": "webdev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "gulp",
    "gulp": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production webpack --mode production --json > utils/stats.json && webpack-bundle-analyzer utils/stats.json build --report utils/report.html --mode static -O && cross-env NODE_ENV=production gulp build",
    "avif": "avif --input='src/img/*.{jpg,jpeg,png}' --output='build/img' --quality=90 --overwrite --verbose && npx avif --input='src/img/_copy/*.{jpg,jpeg,png}' --output='build/img' --quality=90 --overwrite --verbose",
    "format": "prettier --write \"src/{components,js,scss}/**/*.{ts,tsx,js,jsx,vue,scss}\"",
    "lint": "eslint --fix \"src/{components,js}/**/*.{ts,tsx,js,jsx,vue}\"",
    "stylelint": "stylelint --aei --fix \"src/{components,js,scss}/**/*.scss\"",
    "linthtml": "linthtml src/{views,components}/**/*.{html,ejs}"
  },
